{
	"$schema": "https://mintlify.com/docs.json",
	"theme": "mint",
	"name": "AICO",
	"logo": {
		"dark": "/logo/LogoWithText-dark.svg",
		"light": "/logo/LogoWithText-light.svg"
	},
	"favicon": "/favicon.svg",
	"colors": {
		"primary": "#22D3EE",
		"light": "#3DE1E7",
		"dark": "#22D3EE"
	},
	"appearance": {
		"default": "system"
	},
	"navigation": {
		"tabs": [
			{
				"tab": "Documentation",
				"groups": [
					{
						"group": "Getting Started",
						"pages": ["introduction"]
					},
					{
						"group": "Connectors",
						"pages": [
							"connectors/introduction",
							"connectors/quickstart",
							{
								"group": "Brunkhorst",
								"pages": ["connectors/brunkhorst/overview"]
							},
							{
								"group": "PlanSo",
								"pages": ["connectors/reit/overview"]
							}
						]
					}
				]
			},
			{
				"tab": "Brunkhorst API",
				"openapi": "connectors/openapi/brunkhorst-openapi.json"
			},
			{
				"tab": "PlanSo API",
				"openapi": "connectors/openapi/reit-openapi.json"
			}
		],
		"anchors": [
			{
				"anchor": "GitHub",
				"icon": "github",
				"href": "https://github.com/aico-systems",
				"color": {
					"from": "#3DE1E7",
					"to": "#9056FF"
				}
			}
		]
	},
	"navbar": {
		"links": [
			{
				"label": "Support",
				"href": "mailto:support@aicoflow.com"
			}
		],
		"primary": {
			"type": "button",
			"label": "Dashboard",
			"href": "https://app.aicoflow.com"
		}
	},
	"footer": {
		"socials": {
			"github": "https://github.com/aico-systems"
		}
	},
	"api": {
		"playground": {
			"display": "interactive"
		}
	}
}
